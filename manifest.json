  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+boatrental",
      "url": "/en/boat-hire/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/en/upload/",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".gif"]
      }
    }
  ],
  "share_target": {
    "action": "/en/share/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  "permissions": [
    "geolocation",
    "notifications",
    "camera"
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "developer": {
    "name": "Nijenhuis Boat Rental",
    "url": "https://nijenhuis-botenverhuur.com"
  }
} 