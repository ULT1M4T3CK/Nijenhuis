  "shortcuts": [
    {
      "name": "Book a Boat",
      "short_name": "Book",
      "description": "Quick access to boat booking",
      "url": "/en/boat-hire/",
      "icons": [
        {
          "src": "/images/icons/shortcut-book.png",
